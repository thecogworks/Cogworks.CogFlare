{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "8544ac12-23b6-4d0d-82be-00b57cc926e6"
      },
      "ModelsBuilder": {
        "ModelsDirectory": "~/../Cogworks.CogFlare.UI/Models/CmsModels/Generated",
