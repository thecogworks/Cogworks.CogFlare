    "KeyNodes": "1234, 031089",
    "KeyParentNodes": "1077",
    "IsEnabled": true,
    "CacheTimeEdge": "2592000",
    "CacheTime": "0",
    "Domain": "https://www.example.com",
    "BlockAliases": "formBlock",
    "EnableLogging": true,
    "AuthenticationMethod": "Bearer"
  }
